{
  "manifest_version": 2,

  "name": "Snopesit",
  "description": "Search Snopes on the go!",
  "version": "1.0",

  "browser_action": {
      "default_icon": "icon16.png"
  },

  "background" : {
    "scripts" : ["background.js"]
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ]
}
